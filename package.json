{
  "name": "syllascan-monorepo",
  "version": "1.0.0",
  "description": "Syllascan monorepo",
  "workspaces": [
    "apps/*",
    "api/*"
  ],
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "packageManager": "^npm@9.5.1",
  "dependencies": {
    "dotenv-cli": "^7.0.0"
  }
}
